<h2>Login:</h2>
  <%= form_tag '/login' do %>
    <%= label_tag 'E-Mail' %>
    <%= text_field_tag(:email) %>
    <%= label_tag 'Password' %>
    <%= password_field_tag(:password) %>
    <%= submit_tag 'Login' %>
  <% end %>

